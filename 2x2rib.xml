<title>2x2 Rib</title>
<author>Sebastian Greenholtz</author>

<pattern>
  <cast-on>Multiple of 2, plus 2</cast-on>
  <row num="1">
    <knit>2</knit>
    <repeat until="end">
      <purl />
    </repeat>
  </row>
  <row num="2">
    <purl>2</purl>
    <repeat until="end">
      <knit />
    </repeat>
  </row>
  <repeat until="end>
    <row num="1" />
    <row num="2" />
  </repeat>
<pattern>
